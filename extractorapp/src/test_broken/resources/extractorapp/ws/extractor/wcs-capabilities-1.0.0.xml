	<Service version="1.0.0">
		<metadataLink about="http://geoserver.sourceforge.net/html/index.php"
			metadataType="other" />
		<description>This server implements the WCS specification 1.0 and
			1.1.1, it's reference implementation of WCS 1.1.1. All layers
			published by this service are available on WMS also.
     </description>
		<name>WCS</name>
		<label>Web Coverage Service</label>
		<keywords>
			<keyword>WCS</keyword>
			<keyword>WMS</keyword>
			<keyword>GEOSERVER</keyword>
		</keywords>
		<responsibleParty>
			<individualName>Claudius Ptolomaeus</individualName>
			<organisationName>The ancient geographes INC</organisationName>
			<positionName>Chief geographer</positionName>
			<contactInfo>
				<phone />
				<address>
					<city>Alexandria</city>
					<country>Egypt</country>
					<electronicMailAddress>claudius.ptolomaeus@gmail.com</electronicMailAddress>
				</address>
				<onlineResource xlink:href="http://geoserver.org" />
			</contactInfo>
		</responsibleParty>
		<fees>NONE</fees>
		<accessConstraints>NONE</accessConstraints>
	</Service>
	<Capability>
		<Request>
			<GetCapabilities>
				<DCPType>
					<HTTP>
						<Get>
							<OnlineResource xlink:href="http://localhost:8080/geoserver/wcs?" />
						</Get>
					</HTTP>
				</DCPType>
				<DCPType>
					<HTTP>
						<Post>
							<OnlineResource xlink:href="http://localhost:8080/geoserver/wcs?" />
						</Post>
					</HTTP>
				</DCPType>
			</GetCapabilities>
			<DescribeCoverage>
				<DCPType>
					<HTTP>
						<Get>
							<OnlineResource xlink:href="http://localhost:8080/geoserver/wcs?" />
						</Get>
					</HTTP>
				</DCPType>
				<DCPType>
					<HTTP>
						<Post>
							<OnlineResource xlink:href="http://localhost:8080/geoserver/wcs?" />
						</Post>
					</HTTP>
				</DCPType>
			</DescribeCoverage>
			<GetCoverage>
				<DCPType>
					<HTTP>
						<Get>
							<OnlineResource xlink:href="http://localhost:8080/geoserver/wcs?" />
						</Get>
					</HTTP>
				</DCPType>
				<DCPType>
					<HTTP>
						<Post>
							<OnlineResource xlink:href="http://localhost:8080/geoserver/wcs?" />
						</Post>
					</HTTP>
				</DCPType>
			</GetCoverage>
		</Request>
		<Exception>
			<Format>application/vnd.ogc.se_xml</Format>
			<Format>application/vnd.ogc.se_inimage</Format>
			<Format>application/vnd.ogc.se_blank</Format>
		</Exception>
	</Capability>
