<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>

    <rule>
        <from>^/ws/(.*)$</from>
        <to type="forward">/ws/$1</to>
    </rule>
    <rule>
        <from>^/admin/extjs/(.*)$</from>
        <to type="forward">/webjars/extjs/4.1.1a/$1</to>
    </rule>
    <rule>
        <from>^/admin(.*)$</from>
        <to type="forward">/admin$1</to>
    </rule>
    <rule>
        <from>^/resources/(.*)$</from>
        <to type="forward">/$1</to>
    </rule>
    <rule>
        <from>^/(.*)$</from>
        <to type="forward">/ws/$1</to>
    </rule>

</urlrewrite>
