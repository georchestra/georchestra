<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <rule>
        <from>^/$</from>
        <to type="forward">/ws/index.jsp</to>
    </rule>
    <rule>
        <from>^/ws/(.*)$</from>
        <to type="forward">/ws/$1</to>
    </rule>
    <rule>
        <from>^/resources/(.*)$</from>
        <to type="forward">/$1</to>
    </rule>
    <rule>
        <from>^/index.jsp$</from>
        <to type="forward">/ws/index.jsp</to>
    </rule>
</urlrewrite>
